#include <stdio.h>
#include <limits.h> // For INT_MIN

int main() {
    int N;
    scanf("%d", &N);

    int arr[N];
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }

    // Sort the array in descending order
    for (int i = 0; i < N - 1; i++) {
        for (int j = i + 1; j < N; j++) {
            if (arr[i] < arr[j]) {
                int temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
            }
        }
    }

    // Find the second largest unique element
    int largest = arr[0];
    int secondLargest = INT_MIN; // Initialize with the smallest possible integer value

    for (int i = 1; i < N; i++) {
        if (arr[i] < largest) {
            secondLargest = arr[i];
            break; // Found the second largest unique element
        }
    }

    if (secondLargest == INT_MIN) { // If no second largest unique element was found
        printf("No second largest element\n");
    } else {
        printf("%d\n", secondLargest);
    }

    return 0;
}
