#include <stdio.h>

int main() {
    int R, C;

    // Read the number of rows and columns
    scanf("%d %d", &R, &C);

    int matrix1[R][C];
    int matrix2[R][C];
    int sumMatrix[R][C];

    // Read elements of the first matrix
    for (int i = 0; i < R; i++) {
        for (int j = 0; j < C; j++) {
            scanf("%d", &matrix1[i][j]);
        }
    }

    // Read elements of the second matrix
    for (int i = 0; i < R; i++) {
        for (int j = 0; j < C; j++) {
            scanf("%d", &matrix2[i][j]);
        }
    }

    // Calculate the sum matrix
    for (int i = 0; i < R; i++) {
        for (int j = 0; j < C; j++) {
            sumMatrix[i][j] = matrix1[i][j] + matrix2[i][j];
        }
    }

    // Print the sum matrix
    for (int i = 0; i < R; i++) {
        for (int j = 0; j < C; j++) {
            printf("%d", sumMatrix[i][j]);
            if (j < C - 1) {
                printf(" "); // Print space between elements in a row
            }
        }
        printf("\n"); // Move to the next line after printing a row
    }

    return 0;
}
