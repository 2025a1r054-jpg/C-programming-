#include <stdio.h>

int main() {
    int R, C;
    scanf("%d %d", &R, &C);

    int Sum = 0;
    for (int row = 0; row < R; row++) {
        for (int col = 0; col < C; col++) {
            int element;
            scanf("%d", &element);
            Sum += element;
        }
    }
    printf("%d\n",Sum);

    return 0;
}
  
