#include <stdio.h>
int main() {
    int N;
    scanf("%d", &N);

    int arr[N + 1]; 
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }

    int Value, Position;
    scanf("%d", &Value);
    scanf("%d", &Position);
    Position--; 

    for (int i = N; i > Position; i--) {
        arr[i] = arr[i - 1];
    }
    arr[Position] = Value;
    for (int i = 0; i <= N; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");

    return 0;
}
    
